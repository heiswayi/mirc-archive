#selfpro on
ON *:TEXT:*:?: {
  if ($nick isop $comchan($nick,1)) || ($nick isvo $comchan($nick,1)) || (Serv isin $nick) || ($nick == $me) { goto end }
  if ($chr(35) isin $1-) || (/j isin $1-) || (www. isin $1-) || (http:/ isin $1-) || (/server isin $1-) || (.xoom. isin $1-) || (# isin $1-) && ($chan !isin $1-) { .close -m $nick |  .ignore -u15 $mask($fulladdress,3) 
    if ($me isop $comchan($nick,1)) && ($nick ison $comchan($nick,1)) { 
      .raw -q mode $comchan($nick,1) +b $mask($fulladdress,1)
      .raw -q kick $comchan($nick,1) $nick [:Target:] $comchan($nick,1) No Advertising On Private Message [:Kick On:] 5 $fulldate   $sark $fark %xrc 
      .timer 1 120 .ruser inviter $nick
      goto end 
    }
  }
  inc -u3 %queryflod. [ $+ [ $gettok($address,1,64) [ $+ [ $comchan($nick,1) ] ] ] ] 
  if (%queryflod. [ $+ [ $gettok($address,1,64) [ $+ [ $comchan($nick,1) ] ] ] ]  >= 6) { close -m $nick | .ignore -u15 $mask($fulladdress,2) 

    goto end 
  }
  else { goto end }
  :end
}

