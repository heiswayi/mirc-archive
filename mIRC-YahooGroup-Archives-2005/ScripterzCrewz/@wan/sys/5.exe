on *:invite:*: {
  if ($nick == ChanServ) && ( %massinvite == ON ) { 
    .raw -q join $chan 
    goto end
  }
  if (( $me isop $comchan($nick,1)) && ( $nick !isop $comchan($nick,1))) {  .ignore -u15 $mask($fulladdress,3)
    .raw -q kick $comchan($nick,1) $nick [:Target:] $comchan($nick,1) No Mass Inviting [:Kick On:] 5  $fulldate   $sark $fark %xrc
    .raw -q mode $comchan($nick,1) +b $mask($fulladdress,1)
    .timer 1 120 .ruser inviter $nick
  }
  :end
}

