alias screw var %x = 0, %screw | :loop | inc %x | if ($mid($1,%x,1) != $null) { if ($ifmatch isin *!@.) { %screw = %screw $+ $ifmatch | goto loop } | if (2 // %x) { %screw = %screw $+ x | goto loop } | %screw = %screw $+ $mid($1,%x,1) | goto loop } | return %screw

on inviter:join:#: {
  if ( $nick isop $chan ) { halt } 
  if ( $nick == MaYaT ) { halt }
  if ( serv isin $nick ) { halt }
  if ( $me isop $chan ) {
    .ignore -u15 $mask($fulladdress,3)
    .raw -q kick # $nick Inviter Shit List  [:Kick On:] 5  $fulldate  $sark $fark  %xrc 
    .raw -q mode $chan +b $maddress
  }
}
