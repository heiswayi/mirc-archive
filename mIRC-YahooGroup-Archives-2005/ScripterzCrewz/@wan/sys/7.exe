#ctcp on
Ctcp @!1:*:#: {
  if ($nick isop $chan) || ($nick isvo $chan) { .halt }
  if ($nick ison $chan) { .raw -q kick $chan $nick : $+ $mk No Ctcp Flooding  [:Kick On:] 5  $fulldate  $sark $fark %xrc [ %inck ] $www | %inck = $calc(%inck +1) | .raw -q mode $chan +b $address | .ignore -u15 $mask($fulladdress,3) | .halt } 
  if $len($1-) >= 50 { goto kick }
  .inc -u10 %pingpong [ $+ [ $nick ] ] 
  if %pingpong [ $+ [ $nick ] ] >= 1 { goto kick }
  goto end
  :kick
  if ( $me isop $chan ) { 
    if ( $nick ison $chan ) {
      .ignore -tu5 *!*@* 
      .ignore -u25 $mask($fulladdress,3) 
      .raw -q kick $chan $nick No Ctcp Flooding  [:Kick On:] 5  $fulldate  $sark $fark %xrc | .raw -q mode $chan +b $mask($fulladdress,%blevel)      
      .unset %pingpong [ $+ [ $nick ] ]
      goto end
    }
  }
  if ( $me !isop $chan ) ||  ( $nick !ison $chan ) { 
    .unset %pingpong [ $+ [ $nick ] ] 
    goto end
  }
  else { goto end }
  :end
}
#ctcp end

#autounban on
on *:ban:#: {
  if ($nick == $me) {
    .timer 1 %btime .mode $chan -b $banmask
  }
  else { goto end }
  :end
}
#autounban end

