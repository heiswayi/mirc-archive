on ^*:RAWMODE:#:{ haltdef | echo 12 # $timestamp  $chr(2) $+  $chr(3) $+  $chr(49) $+ $chr(52) $chr(155) $+  $chr(155)  $+  $chr(3) $+  $chr(49) $+ $chr(53) $+  $chr(155)  $+  $chr(155)  $chr(15) 4( 14¤ 4) 14Channel mode:14 $nick $+  sets mode 4(14 $1 4)14 $2- }
on ^*:kick:#: {
  haltdef
  if ($knick == $me) && ($nick != $me) { echo 15 $timestamp $chr(2) $+ $chr(3) $+ $chr(49) $+ $chr(52) $chr(155) $+ $chr(155) $+ $chr(3) $+ $chr(49) $+ $chr(53) $+ $chr(155) $+ $chr(155) $chr(15) 4 You were kicked from $chan by $nick 4(14 $+ $address $+ 4) 4(14 $+ $1- 4) | unset %op. [ $+ [ $chan ] ] | goto rejoin }
  if ($nick == $me) { goto kickout } 
  inc -u1 %masskick- [ $+ [ $nick ] $+ [ $chan ] ]
  if (%masskick- [ $+ [ $nick ] $+ [ $chan ] ] == %masskickcount) && ($me isop #) && (%channelmasskick == on) { kick $chan $nick $read $mircdircrack\masskick.txt | goto end } 
  :kickout
  echo # 15 $timestamp $chr(2) $+ $chr(3) $+ $chr(49) $+ $chr(52) $chr(155) $+ $chr(155) $+ $chr(3) $+ $chr(49) $+ $chr(53) $+ $chr(155) $+ $chr(155) $chr(15) 4( 15¤ 4) 14Kicks:4 $knick was kicked by $nick 4(14 $+ $1- 4) 
  if ($istok(%kicker,$knick,44)) { %kicker = $remtok(%kicker,$knick,1,44) }
  goto end
  :rejoin
  trigger:rev $nick $wildsite
  if (%rejoinkick == on) { goto rejoinonly }
  if (%rejoinrevengekick == on) { goto rejoinwithrevenge }
  if (%rejoinrevengeshitlistkick == on) { goto rejoinrevengeshitlist }
  else goto end
  :rejoinonly
  if (($active != #) && (Status?Window !iswm $active)) { echo -a 15 $timestamp $chr(2) $+ $chr(3) $+ $chr(49) $+ $chr(52) $chr(155) $+ $chr(155) $+ $chr(3) $+ $chr(49) $+ $chr(53) $+ $chr(155) $+ $chr(155) $chr(15) 4( 15x 4)14 Now attempting to rejoin4 # $+ . }
  join # | goto end
  :rejoinwithrevenge
  %kicker = $addtok(%kicker,$nick,44)
  %shitlist = $addtok(%shitlist,$nick,44)
  join # | goto end
  :rejoinrevengeshitlist
  %kicker = $addtok(%kicker,$nick,44)
  %shitlist = $addtok(%shitlist,$nick,44)
  .auser -a shitlist $wildsite
  .timerrejoinrevengeshitlist 1 5 chanserv invite $chan 
  :end
}
on *:ban:#: {
  if ($me !isop $chan) { goto end }
  if ($nick != $me) {
    inc -u1 %massban- [ $+ [ $nick ] $+ [ $chan ] ] 
    if (%massban- [ $+ [ $nick ] $+ [ $chan ] ] == %massbancount) && (%channelmassban == on) { .auser domain $address($nick,3) | kick $chan $nick $read $mircdircrack\massban.txt | ban -u30 # $nick 11 | goto end }
