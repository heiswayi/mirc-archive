on *:dialog:pingsys:*:*:{
  if ($devent == init) {
    update.pingchannel
    if (%autoping == $null) { set %autoping off }
    if (%autoping == on) { did -c pingsys 5 }
    if (%pingtrigger != $null) { did -a pingsys 7 %pingtrigger }
  }
  if ($devent == edit) {
    if ($did == 8) { set %pingtempchan $did(8).text }
    if ($did == 7) { set %pingtrigger $did(7).text }
  }
  if ($devent == sclick) {
    if ($did == 3) { _webteam }
    if ($did == 8) { set %setup.pingchannel.selected $did($dname,8,$did(8).sel) }
    if ($did == 9) {
      set %pingchannel %pingchannel $+ %pingtempchan $+ ,
      update.pingchannel
    }
    if ($did == 10) {
      set -u1 %setup.rc %setup.pingchannel.selected $+ ,
      set %pingchannel $remove(%pingchannel,%setup.rc)
      update.pingchannel
    }
    if ($did == 11) { unset %pingchannel | update.pingchannel }
    if ($did == 5) {
      if (%autoping == on) { set %autoping off | halt }
      if (%autoping == off) { set %autoping on | halt }
    }
  }
}
alias -l update.pingchannel {
  did -r pingsys 8
  set %temp.total $count(%pingchannel,$chr(44))
  set %temp.count 0
  :start
  inc %temp.count 1
  did -a pingsys 8 $gettok(%pingchannel,%temp.count,44)
  if (%temp.count < %temp.total) { goto start }
}
on *:text:*:%pingchannel: {
  if (%autoping == on) {
    if (%pingflood- [ $+ [ $nick ] $+ [ $chan ] ] == 2) { .notice $nick Wait after 1 minute to perform my %pingtrigger command. | .ignore -u60  $wildsite | goto end  } 
    if (%pingtrigger == $1) && ($2 == $null) { .notice $nick $logo $+ : correct usage: %pingtrigger <nick> ie. %pingtrigger $nick | inc -u30 %pingflood- [ $+ [ $nick ] $+ [ $chan ] ] | halt }
    if (%pingtrigger == $1) && ($2 == $me) { ignore -u30 $nick | inc -u30 %pingflood- [ $+ [ $nick ] $+ [ $chan ] ] | halt }
    if (%pingtrigger == $1) && ($2 == $nick) { pong | inc -u30 %pingflood- [ $+ [ $nick ] $+ [ $chan ] ] | halt }
  }
  :end
}
