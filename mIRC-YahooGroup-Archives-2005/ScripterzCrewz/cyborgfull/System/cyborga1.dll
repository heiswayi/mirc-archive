  button "config" 3, 70 37 17 10,
  button "Design Color" 4, 7 12 50 10,
  button "Message Color" 5, 7 25 50 10,
  text "Design 1:" 6, 60 12 25 10,
  text "Design 2:" 7, 60 25 25 10,
  edit "" 8, 87 12 25 10, AutoHs
  edit "" 9, 87 25 25 10, AutoHs
  button "Finish" 10, 58 50 25 10, Ok  
  check "background color" 11, 7 38 55 8,
  button "Default" 12, 30 50 25 10,
}
on *:DIALOG:input4:sclick:*: {
  if ($did == 3) { dialog -m colorb3 colorb3 }
  if ($did == 4) { dialog -m colorb1 colorb1 }
  if ($did == 5) { dialog -m colorb2 colorb2 }
  if ($did == 12) { dialog -m colorb2 colorb2 }
  if ($did == 11) {
    if (%colorback == on) { set %colorback off | halt }
    if (%colorback == off) { set %colorback on | halt }
  }
}
on *:DIALOG:input4:edit:*: {
  if ($did == 8) { set %textdesign1 $did(8).text }
  if ($did == 9) { set %textdesign2 $did(9).text }
}
on *:DIALOG:input4:init:0: {
  if (%textdesign1 != $null) { did -a input4 8 %textdesign1 }
  if (%textdesign2 != $null) { did -a input4 9 %textdesign2 }
  if (%colorback == $null) { set %colorback off }
  if (%colorback == on) { did -c input4 11 }
}
