#lookuser on
on *:quit:  { if ($nick != $me) { writeini -n seen\users.ini $nick info $ctime | halt } }
on *:part:*: { if ($chan isin %seenchannel) && ($nick != $me) { /writeini -n seen\users.ini $nick info $ctime | halt } }
#lookuser end
on *:text:*:%seenchannel: {
  if (%seensystem == on) {
    inc -u60 %seenflood- [ $+ [ $nick ] $+ [ $chan ] ] 
    if (%seenflood- [ $+ [ $nick ] $+ [ $chan ] ] == 3) { .notice $nick Wait after 1 minute to perform my %seentrigger command. | .ignore -u60  $wildsite | goto end } 
    if (%seentrigger == $1) && ($2 == $null) { msg $chan $logo $nick $+ : correct usage: %seentrigger <nick> ie. %seentrigger $nick | goto end }
    if (%seentrigger == $1) && ($2 == $me) {
      if ($me isop $chan) { if (%seenflood- [ $+ [ $nick ] $+ [ $chan ] ] < 2) { msg $chan $logo Hei.. $nick Are you such a lazy person? } | else { kick # $nick Im just here and kicking you...lol } | goto end }
      if ($me !isop $chan) { msg # $logo $nick $+ : Im just here and. | goto end }
    }
    if (%seentrigger == $1) && ($2 == $nick) {
      if ($me isop $chan) { if (%seenflood- [ $+ [ $nick ] $+ [ $chan ] ] > 1) { kick $chan $nick Stop playing with my %seentrigger command $nick $+ . } | else { msg $chan $logo Hei.. $nick Are you really stupid, looking for yourself? } | goto end }
      if ($me !isop $chan) { msg $chan $logo $nick $+ : Stop playing with my %seentrigger command $nick $+ . | goto end }
    }
    if (%seentrigger == $1) && ($2 ison $chan) { msg $chan $logo $nick $+ : Just look for $2 by your self in # $+ . | goto end }
    if (%seentrigger == $1) && ($readini seen\users.ini $2 info == $null) { msg $chan $logo $nick $+ :  no matches found for $2 $+ . | goto end }
    if (%seentrigger == $1) && ($readini seen\users.ini $2 info != $null) { set %savetime $readini seen\users.ini $2 info | set %duration $calc($ctime - %savetime) | msg $chan $logo $nick $+ : $2 was last seen $duration(%duration) ago. | goto end }
  }
  :end
}
dialog seen {
  title "Seen system"
  size -1 -1 137 119
  icon Miscellaneous\seen.ico
  option dbu
  text "Cyborg Full-Script Edition" 2, 35 1 60 7, Center   Disable
  link "www.Brain-Crash.com" 3, 39 9 110 14,
  box "seen set-up" 4, 2 17 132 100, Disable
  check "enable seen system" 5, 7 31 62 7,
  text "seen trigger:" 6, 7 41 30 7,
  edit "" 7, 37 40 40 10,
  button "view seen logs" 8, 82 32 45 10,
  button "erase seen logs" 9, 82 42 45 10,
  combo 10, 7 52 70 65,
  button "add channel" 11, 82 70 45 10,
  button "remove channel" 12, 82 80 45 10,
  button "reset channels" 13, 82 90 45 10,
  button "save" 14, 82 100 45 10, Ok  
}
on *:dialog:seen:*:*:{
  if ($devent == init) {
    update.seenchannel
    if (%seensystem == $null) { .disable #lookuser | did -b $dname 7 | did -b $dname 10 | set %seensystem off }
    if (%seensystem == on) { .enable #lookuser | did -e $dname 7 | did -e $dname 10 | did -c seen 5 }
    if (%seentrigger != $null) { did -a seen 7 %seentrigger }
  }
  if ($devent == edit) {
    if ($did == 10) { set %seentempchan $did(10).text }
    if ($did == 7) { set %seentrigger $did(7).text }
  }
  if ($devent == sclick) {
    if ($did == 3) { _webteam }
    if ($did == 10) { set %setup.seenchannel.selected $did($dname,10,$did(10).sel) }
    if ($did == 11) {
      set %seenchannel %seenchannel $+ %seentempchan $+ ,
      update.seenchannel
    }
    if ($did == 12) {
      set -u1 %setup.rc %setup.seenchannel.selected $+ ,
      set %seenchannel $remove(%seenchannel,%setup.rc)
      update.seenchannel
    }
    if ($did == 13) { unset %seenchannel | update.seenchannel }
    if ($did == 5) {
      if (%seensystem == on) { set %seensystem off | .disable #lookuser | did -b $dname 7 | did -b $dname 10  | halt }
      if (%seensystem == off) { set %seensystem on | .enable #lookuser | did -e $dname 7 | did -e $dname 10 | halt }
    }
    if ($did == 8) { run seen\users.ini }
    if ($did == 9) { write -c seen\users.ini }
  }
}
alias -l update.seenchannel {
  did -r seen 10
  set %temp.total $count(%seenchannel,$chr(44))
  set %temp.count 0
  :start
  inc %temp.count 1
  did -a seen 10 $gettok(%seenchannel,%temp.count,44)
  if (%temp.count < %temp.total) { goto start }
}
