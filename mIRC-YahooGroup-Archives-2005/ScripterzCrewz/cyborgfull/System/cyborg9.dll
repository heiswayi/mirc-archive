alias setDCCGet.Browse {
  window -c @DCC-Get
  window -akl -t15,40,50,70 @DCC-Get  
  titlebar @DCC-Get received files
  clear @DCC-Get
  aline @DCC-Get 15,2 From $chr(9) 15,2 Filename $chr(9) 15,2 Size $chr(9) 15,2 Date $chr(9) 15,2 Original Path 
  var %nCounter = 0
  var %cDownloadPath = $mircdir $+ Download\
  while (%nCounter < $findfile(%cDownloadPath,*.*,0)) {
    inc %nCounter
    var %cPathExtract = $findfile(%cDownloadPath,*.*,%nCounter)
    var %cFileFolder = $gettok(%cPathExtract,$calc($numtok(%cPathExtract,92)-1),92)
    var %cLineInfo = %cFileFolder $chr(9) 4 $+ $nopath(%cPathExtract) $chr(9) $int($calc($file(%cPathExtract).size / 1024)) $+ kb $chr(9) $asctime($file(%cPathExtract).mtime) $chr(9) $+ %cPathExtract
    aline @DCC-Get %cLineInfo
  }
  window -b @DCC-Get
  sline @DCC-Get 1
}
menu @DCC-Get {
  dclick : if ($sline(@DCC-Get,1).ln > 1) .run $setDCCGet.Browse.Target
  $iif($sline(@DCC-Get,1).ln > 1,Open Selected) : .run $setDCCGet.Browse.Target
  -
  $iif($sline(@DCC-Get,1).ln > 1,Copy to ...) : { 
    var %cTempPath = $$sdir="¤()CYB()ORG()¤  - Copy $nopath($setDCCGet.Browse.Target) to folder ...?" $mircdir
    copy $lookDCCGet.Browse.Target " $+ %cTempPath $+ $nopath($lookDCCGet.Browse.Target) $+ "
  }
  $iif($sline(@DCC-Get,1).ln > 1,Move to ...) : { 
    var %cTempPath = $$sdir="¤()CYB()ORG()¤  - Move $nopath($setDCCGet.Browse.Target) to folder ...?" $mircdir
    rename $setDCCGet.Browse.Target " $+ %cTempPath $+ $nopath($lookDCCGet.Browse.Target) $+ "
    dline @DCC-Get $sline(@DCC-Get,1).ln
  }
  $iif($sline(@DCC-Get,1).ln > 1,Delete)
  .Delete " $+ $strip($gettok($sline(@DCC-Get,1),2,9)) $+ " now! : { 
    delete $setDCCGetwse.Target
    dline @DCC-Get $sline(@DCC-Get,1).ln
  }
  -
  Refresh list : setDCCGet.Browse
  -
  Close Browser : window -c @DCC-Get
}
