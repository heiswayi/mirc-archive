alias -l tread if ($ini(crack\help.txt,$1)) loadbuf -oert $+ $1 help 8 crack\help.txt | else did -ra help 8 $str($crlf,3) $+ $str($chr(160),30) $+ ***  File Not found ***
alias -l cread if ($ini(crack\help2.txt,$1)) loadbuf -oert $+ $1 help 12 crack\help2.txt | else did -ra help 12 $str($crlf,3) $+ $str($chr(160),30) $+ *** File Not Found ***
on *:dialog:help:*:*:{
  if ($devent == init) {
    set %cdsp 1
    didtok help 7 46 About. .Channel Protections.Settings.Agent Spy Bot.Away System.Connect.Mp3 Player System.User level.Anti Inviter.Ping System.Seen System.Scans.Background.Auto Op/Voice.Script triggers.Autothanks.Autotalk and Greets.Querytalk.Games.DALnet Services. .FKeys. . .
    didtok help 11 46 Commands. .a.alist.aping.aquery.atopic.awaynick.b.backaway.chat.chnick.cq.cs.csp.dop.dv.edit.help.findclone.friend.identify.insult.j.k.kb.mp3.ms.msgaway.n.ns.o.op.p.ping.q.seen.send.talk.thanks.time.v.version.w.windows.
    did -ra help 8 $str($crlf,3) $+ $str($chr(160),30) $+ *** No topic selected ***
    did -ra help 12 $str($crlf,3) $+ $str($chr(160),30) $+ *** No command selected *** 
  }
  if ($devent == sclick) {
    if ($did == 3) { _webteam }
    if ($did == 4) { helpup }
    if ($did == 7) { if ($did(7).seltext == $chr(160)) { did -ra help 8 $str($crlf,3) $+ $str($chr(160),30) $+ *** No topic selected *** | return } | var %t = $gettok($did(7).seltext,1,32) | tread %t }
    if ($did == 11) { if ($did(11).seltext == $chr(160)) { did -ra help 12 $str($crlf,3) $+ $str($chr(160),30) $+ *** No command selected *** | return } | var %oc = $gettok($did(11).seltext,1,32) | cread %oc }
  }
}
