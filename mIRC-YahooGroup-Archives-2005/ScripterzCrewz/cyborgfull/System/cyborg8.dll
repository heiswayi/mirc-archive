on *:CTCPREPLY:ping*: {
  if  ($nick == nickserv) || ($nick == memoserv) goto end   
  if (chanserv == $nick) { 
    set %clag $calc($ctime - $2)   
    if (%clag <= 30) && (%ON == OFF) {
      .set %ON ON | set %idle 0 | .timerSERvices OFF | chanservnormallag      
      .timerPING 1 300 .ctcp chanserv@services.dal.net PING 
      halt   
    }
    elseif (%ON == ON) { .timerPING 1 300 .ctcp chanserv@services.dal.net PING | .timerCheckPING 1 300 CheckPing | halt  
    }
    if (%ON == OFF) { chantoolagserv
      .timerPING 1 300 .ctcp chanserv@services.dal.net PING 
      halt   
    }
    elseif (%clag >= 60) && (%ON == ON) {
      .set %ON OFF | chanlagserv
      .timerPING 1 300 .ctcp chanserv@services.dal.net PING 
      .timerservices 0 300 ChanLaG  
      halt   
    }
  }
  var %pt = $calc($ctime - $2)  
  if ($nick == nickserv) || ($nick == memoserv) goto end   
  .notice $nick $logo Ping Reply: $duration(%pt) 
  if (%pt < 3) .notice $nick $logo Ping Comment: Good
  elseif (%pt < 10) .notice $nick $logo Ping Comment: Bad
  else .notice $nick $logo Ping Comment: Worst
  :end
}
alias pong  { .notice $nick $logo Wait for your 4PING request.. | .ctcp $nick ping | halt }
alias ctcptime {
  if (%ctcptime  == $null ) { 
  .ctcpreply $nick TIME %ctcptime }
  else { .ctcpreply $nick TIME %logo $timectcp }
}
alias ctcpping {
  if (%ctcpping  == $null ) { .ctcpreply $nick PING %ctcpping }
  else { .ctcpreply $nick Ping $pingctcp %logo }
}
alias ctcpversion {
  if (%Version  == $null ) { .ctcpreply $nick VERSION $vctcp }
  else { .ctcpreply $nick VERSION $versionctcp }
}
alias ctcpfinger {
  if (%finger  == $null ) { .ctcpreply $nick Finger $fctcp }
  else { .ctcpreply $nick FINGER $fingerctcp %logo }
}
alias ctcpclientinfo {
  if (%Version  == $null ) { .ctcpreply $nick CLIENTINFO $pctcp }
  else { .ctcpreply $nick CLIENTINFO $pingctcp %logo }
}
alias ctcpuserinfo {
  if (%Version  == $null ) { .ctcpreply $nick USERINFO $pctcp }
  else { .ctcpreply $nick USERINFO $pingctcp %logo }
}
alias timectcp /return Get The Cyborg Full-Script Edition @ $team:web
alias pingctcp /return  No Reply:Cyborg Full-Script Edition @ $team:web
alias versionctcp /return Cyborg Full-Script  Edition by $master mIRC Protection Script Engine! 
alias fingerctcp /return Cyborg Full-Script Edition @ $team:web
alias tctcp /return Its Time To Get Cyborg Full-Script Edition @ $team:web
alias pctcp /return No Reply: Cyborg Full-Script Edition $team:web
alias vctcp /return Cyborg Full-Script Edition by $master In-Search of Excellence! 
alias fctcp /return Cyborg Full-Script Edition $mymail
#autogreet off
on !*:join:%textjoinchannel: {
  if ($nick == %spybot) { goto end }
  if (%partspy. [ $+ [ $+($chan,$wildsite) ] ] > %partspycount) { goto end }
  inc %joined $+ $nick 
  if (%joined [ $+ [ $nick ] ] == 1) .notice $nick $logo %joingreetmsg
  else .notice $nick $logo Welcome Back $nick $+ .  I've seen you %joined [ $+ [ $nick ] ] $+ x. 
  :end
}
on !*:part:%textjoinchannel:{ 
  if ($nick == %spybot) { goto end }
  inc -u30 %partspy. [ $+ [ $+($chan,$wildsite) ] ]
  .msg $nick $logo %partgreetmsg
  :end
}
#autogreet end
alias helpset { dialog -m help help  | helpopen }
alias helpclose {
  :reduce
  if (%cdsp <= 10) { dialog -c help | halt }
  dec %cdsp 3 | dialog -s help -1 -1 484 %cdsp | goto reduce
}
alias helpopen {
  :increase
  if (%cdsp >= 354) { dialog -s help -1 -1 484 354  | halt }
  inc %cdsp 3 | dialog -s help -1 -1 484 %cdsp | goto increase
}
alias helpup { set %cdsp 354 | dialog -s help -1 -1 484 354 | .timerhelpclose 1 0 helpclose }
dialog help {
  title "Cyborg Script Help"
  size -1 -1 242 177
  option dbu
  text "Cyborg Full-Script Edition" 2, 67 1 110 7, Center   Disable
