on *:OPEN:?:{
  if ( %querylimits == Off ) { goto open1 }
  if ($nick == $me) || ($nick isop $comchan($nick,1)) { closemsg $nick | goto end }
  if ($nick == %spybot) { haltdef | goto end }
  if (($nick == %friend1) || ($nick == %friend2) || ($nick == %friend3) || ($nick == %friend4) || ($nick == %friend5) || ($nick == %friend6) || ($nick == %friend7) || ($nick == %friend8) || ($nick == %friend9) || ($nick == %friend10) || ($nick == %friend11) || ($nick == %friend12) || ($nick == %friend13) || ($nick == %friend14) || ($nick == %friend15) || ($nick == %friend16) || ($nick == %friend17) || ($nick == %friend18) || ($nick == %friend19) || ($nick == %friend20) || ($nick == %friend21) || ($nick == %friend22) || ($nick == %friend23) || ($nick == %friend24) || ($nick == %friend25) || ($nick == %friend26) || ($nick == %friend27) || ($nick == %friend28) || ($nick == %friend29) || ($nick == %friend30)) { goto open1 }
  if (%queryfldmsg == on) {
    inc -u1 %queryfloodmsg
    if (%queryfloodmsg == %queryfloodmsgcount) { set %fromflood Query | protfld1 | goto end } 
  }  
  if ($query(0) = %querylimit) { .echo -a 1211 4Warning: 15Your query limit is exceeded. All incoming queries will be ignore.... } 
  if ($query(0) > %querylimit) { 
    .ignore -pu20 $wildsite 
    .close -m $nick
    set -u20 %querylimitnicks $nick
    .MSG $nick $logo $+  $chr(15) Sorry, I have my limit on my query... 
    halt 
  }
  :open1
  set %c $null | var %i = 0 | :loop | inc %i 
  if ($comchan($nick,0) < %i) { echo $nick $chr(2) $+  $chr(3) $+  $chr(49) $+ $chr(52) $chr(155) $+  $chr(155)  $+  $chr(3) $+  $chr(49) $+ $chr(53) $+  $chr(155)  $+  $chr(155)  $chr(15) $chr(3) $+ $chr(49) $+ $chr(52) $+ Query with $nick 4(14 $+ $address $+ 4) 15 $time(h:nntt)  | if (%c == $null) { set %c none } | echo $nick $chr(2) $+  $chr(3) $+  $chr(49) $+ $chr(52) $chr(155) $+  $chr(155)  $+  $chr(3) $+  $chr(49) $+ $chr(53) $+  $chr(155)  $+  $chr(155)  $chr(15) $chr(3) $+ $chr(49) $+ $chr(52) $+ Common channels  4(15 $+ %c $+ 4) | halt }
  else { set %c %c $comchan($nick,%i) | goto loop }
  :end
}
alias protfld {
  closemsg $nick
  ignore -u $+ %silenceon $wildsite  
  echo -a $timestamp $chr(2) $+ $chr(3) $+  $chr(49) $+ $chr(52) $chr(155) $+ $chr(155)  $+ $chr(3) $+ $chr(49) $+ $chr(53) $+  $chr(155)  $+  $chr(155)  $chr(15) 4 $+ %fromflood flood 4: $nick 4(15 $+ $address $+ 4)
}
alias protfld1 {
  ignore -pntiu $+ %silenceon *!*@*  
  echo -a $timestamp $chr(2) $+ $chr(3) $+ $chr(49) $+ $chr(52) $chr(155) $+ $chr(155) $+ $chr(3) $+ $chr(49) $+ $chr(53) $+ $chr(155)  $+ $chr(155)  $chr(15) 4Multiple %fromflood flood 4: $nick 4(15 $+ $address $+ 4)
}
ctcp *:*: {
  if (Send isin $1-) { goto end }
  if (get isin $1-) { goto end }
  if (Chat isin $1-) { goto end }
  if (Dcc isin $1-) { goto end }
  if (resume isin $1-) { goto end }
  haltdef
  if (%ctcpfldmsg == on) {
    inc -u1 %ctcpfloodmsg- [ $+ [ $wildsite ]  ]
    if (%ctcpfloodmsg- [ $+ [ $wildsite ]  ] > %ctcpfloodmsgcount) { set %fromflood Ctcp's | protfld1 | goto protect } 
    if (%ctcpfloodmsg- [ $+ [ $wildsite ]  ] == %ctcpfloodmsgcount) { set %fromflood Ctcp's | protfld | goto protect }
    :ctcpecho
    if (Sound isin $1-) { set %ctcpechonow SOUND | goto ctcp }
    if (page isin $1-) { set %ctcpechonow PAGE14: $2- $+ : | goto ctcp }
    if (Ping isin $1-) { set %ctcpechonow PING | ctcpping | goto ctcp }
    if (Version isin $1-) { set %ctcpechonow VERSION | ctcpversion | goto ctcp }
    if (FINGER isin $1-) { set %ctcpechonow FINGER | ctcpfinger | goto ctcp }
    if (TIME isin $1-) { set %ctcpechonow TIME | ctcptime | goto ctcp }
    if (ClientInfo isin $1-) { set %ctcpechonow CLIENTINFO | ctcpclientinfo | goto ctcp }
    if (UserINFO isin $1-) { set %ctcpechonow USERINFO | ctcpuserinfo | goto ctcp }
    else goto end
  }
  :ctcp
  if ($chan isin $1-) { if (!%_ctcp) { set -u10 %_ctcp ! | echo # 12 $timestamp $chr(2) $+ $chr(3) $+  $chr(49) $+ $chr(52) $chr(155) $+  $chr(155)  $+  $chr(3) $+  $chr(49) $+ $chr(53) $+  $chr(155)  $+  $chr(155)  $chr(15)  11Ctcp:4$nick 4(14 $+ $address $+ 4)14 trying to 4 %ctcpechonow 14 $chan } | goto channelctcp }
  if (!%_ctcp) set -u10 %_ctcp ! | echo -a $chr(2) $+  $chr(3) $+ $chr(49) $+ $chr(52) $chr(155) $+  $chr(155)  $+  $chr(3) $+  $chr(49) $+ $chr(53) $+  $chr(155)  $+  $chr(155)  $chr(15) 4Ctcp:4 %ctcpechonow 14requested by $nick 4(14 $+ $address $+ 4)14 | goto end
  :protect
  set %i 0 
  :nexti 
  inc %i 
  if $comchan($nick,0) < %i { unset %i | return } 
  if ($me !isop $comchan($nick,%i)) || ($nick isop $comchan($nick,%i)) { .ignore -u60 $wildsite | echo -a $chr(2) $+  $chr(3) $+  $chr(49) $+ $chr(52) $chr(155) $+ $chr(155) $+ $chr(3) $+ $chr(49) $+ $chr(53) $+ $chr(155) $+ $chr(155)  $chr(15) 4Ctcp:4 FLOOD 14detected  from 4(14 $+ $wildsite $+ 4)14 | goto end }
  else { ban -u20 $comchan($nick,%i) $nick 11 | kick $comchan($nick,%i) $nick Ctcp Flood | goto nexti 
  }
  :channelctcp
  if (%massctcp == on) {
    .set %reportispamnick $nick
    var %i = 0
    while (%i < $comchan(%reportispamnick,0)) { 
      inc %i
      if (%reportispamnick !isop $comchan(%reportispamnick,%i)) { goto ctcpganstah }
      else goto end 
      :ctcpganstah
      if ($me !isop $comchan(%reportispamnick,%i)) { goto end }
      ban -u $+ %inviters $comchan(%reportispamnick,%i) %reportispamnick 11
      kick $comchan(%reportispamnick,%i) %reportispamnick Mass4 %ctcpechonow Ctcp | goto end
    }  
  }
  :end
}
