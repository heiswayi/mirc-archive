on *:DIALOG:user:*:*:{
  if ($devent == init) {
    update.friendlist
    update.kicklist
    update.shitlist
    update.ignorelist
  }
  if ($devent == edit) {
    if ($did == 9) { set %tempfriend $did(9).text }
    if ($did == 13) { set %tempkick $did(13).text }
    if ($did == 22) { set %tempshit $did(22).text }
    if ($did == 30) { set %tempignore $did(30).text }
  }
  if ($devent == sclick) {
    if ($did == 3) { _webteam }
    if ($did == 11) { set %setup.friendlist.selected $did($dname,11,$did(11).sel) | did -r user 9 | did -a user 9 $did($dname,11,$did(11).sel) }
    if ($did == 6) {
      set %friendlist %friendlist $+ %tempfriend $+ ,
      update.friendlist
      did -r user 9
      .auser friend %tempfriend
    }
    if ($did == 7) {
      set -u1 %setup.rc %setup.friendlist.selected $+ ,
      set %friendlist $remove(%friendlist,%setup.rc)
      update.friendlist
      did -r user 9
      .ruser friend %setup.friendlist.selected
    }
    if ($did == 8) { unset %friendlist | update.friendlist | did -r user 9 | .rlevel friend }
    if ($did == 15) {
      set %kicklist %kicklist $+ %tempkick $+ ,
      update.kicklist
      did -r user 13
      .auser bullshit %tempkick
    }
    if ($did == 16) {
      set -u1 %setup.rc %setup.kicklist.selected $+ ,
      set %kicklist $remove(%kicklist,%setup.rc)
      update.kicklist
      did -r user 13
      .ruser bullshit %setup.kicklist.selected
    }
    if ($did == 17) { unset %kicklist | update.kicklist | did -r user 13 | .rlevel bullshit }
    if ($did == 18) { set %setup.kicklist.selected $did($dname,18,$did(18).sel) | did -r user 13 | did -a user 13 $did($dname,18,$did(18).sel) }
    if ($did == 19) { run crack\kicklisted.txt }
    if ($did == 23) {
      set %shitlist $addtok(%shitlist,$gettok(%tempshit,1,33),44)
      update.shitlist
      did -r user 22
      .auser shitlist %tempshit
    }
    if ($did == 24) {
      set -u0 %setup.rc %setup.shitlist.selected
      set %shitlist $remove(%shitlist,%setup.rc)
      update.shitlist
      did -r user 22
      .ruser shitlist %setup.shitlist.selected
    }
    if ($did == 25) { unset %shitlist | update.shitlist | did -r user 22 | .rlevel shitlist }
    if ($did == 27) { set %setup.shitlist.selected $did($dname,27,$did(27).sel) | did -r user 22 | did -a user 22 $did($dname,27,$did(27).sel) }
    if ($did == 26) { run Text\kick.txt }
    if ($did == 31) {
      set %ignorelist %ignorelist $+ %tempignore $+ ,
      update.ignorelist
      did -r user 30
      .ignore %tempignore
    }
    if ($did == 32) {
      set -u1 %setup.rc %setup.ignorelist.selected $+ ,
      set %ignorelist $remove(%ignorelist,%setup.rc)
      update.ignorelist
      did -r user 30
      .ignore -r %setup.ignorelist.selected
    }
    if ($did == 33) { set %setup.ignorelist.selected $did($dname,33,$did(33).sel) | did -r user 30 | did -a user 30 $did($dname,33,$did(33).sel) }
    if ($did == 36) { .rlevel friend }
    if ($did == 37) { .rlevel bullshit }
    if ($did == 38) { .rlevel shitlist }
    if ($did == 39) { .rlevel domain | .rlevel inviter | .rlevel domains }
    if ($did == 40) { .rlevel ginviter }
  }
}
alias -l update.kicklist {
  did -r user 18
  set %temp.total $count(%kicklist,$chr(44))
  set %temp.count 0
  :start
  inc %temp.count 1
  did -a user 18 $gettok(%kicklist,%temp.count,44)
  if (%temp.count < %temp.total) { goto start }
}
alias -l update.friendlist {
  did -r user 11
  set %temp.total $count(%friendlist,$chr(44))
  set %temp.count 0
  :start
  inc %temp.count 1
  did -a user 11 $gettok(%friendlist,%temp.count,44)
  if (%temp.count < %temp.total) { goto start }
}
alias -l update.shitlist {
  did -r user 27
  set %temp.total $count(%shitlist,$chr(44))
  set %temp.count 0
  :start
  inc %temp.count 1
  did -a user 27 $gettok(%shitlist,%temp.count,44)
  if (%temp.count < %temp.total) { goto start }
}
alias -l update.ignorelist {
  did -r user 33
  set %temp.total $count(%ignorelist,$chr(44))
  set %temp.count 0
  :start
  inc %temp.count 1
  did -a user 33 $gettok(%ignorelist,%temp.count,44)
  if (%temp.count < %temp.total) { goto start }
}

