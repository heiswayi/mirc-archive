    if (%spambots == on) { did -c inviter 11 }
    if (%ai3 == $null) { set %ai3 off }
    if (%ai3 == on) { did -c inviter 12 }
    if (%common == $null) { set %common off }
    if (%common == on) { did -c inviter 13 }
