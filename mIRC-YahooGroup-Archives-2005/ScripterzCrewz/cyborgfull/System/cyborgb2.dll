  ; if (%kickreply != $null) { did -a thanks 16 %kickreply }
}
on *:DIALOG:thanks:sclick:*: {
  if ($did == 17) {
    did -r thanks 11 | did -r thanks 12 | did -r thanks 13 | did -r thanks 14 | did -r thanks 15
    set %opreply Waww!... thanks $nick for +o $+ . | did -a thanks 11 %opreply
    set %deopreply Im so sad for removin my +o $+ . $nick | did -a thanks 12 %deopreply
    set %voicereply Wow! thanks for +v $nick $+ . | did -a thanks 13 %voicereply
    set %devoicereply Hey $nick pls gimme back my +v | did -a thanks 14 %devoicereply
    set %banreply Please don't ban me | did -a thanks 15 %banreply
    ;set %kickreply Why did you kicked me? isn't me cute... | did -a thanks 16 %kickreply
  }
}
