dialog input7 {
  title "Custom Nick Completer"
  size -1 -1 117 55
  icon Miscellaneous\paint.ico
  option dbu
  box "Custom design" 2, 3 2 112 51, Disable
  button "Color 2" 3, 7 37 50 10,
  button "Design Color" 4, 7 12 50 10,
  button "Color 1" 5, 7 25 50 10,
  text "Left Design" 6, 60 12 32 10,
  text "Right Design" 7, 60 25 32 10,
  edit "" 8, 92 12 15 10,
  edit "" 9, 92 25 15 10,
  button "Finish" 10, 87 37 25 10, Ok  
  check "Rand" 11, 60 37 25 10,
}
on *:DIALOG:input7:sclick:*: {
  if ($did == 3) { dialog -m colord2 colord2 }
  if ($did == 4) { dialog -m colord3 colord3 }
  if ($did == 5) { dialog -m colord1 colord1 }
  if ($did == 11) {
    if (%colord3rand == on) { set %colord3rand off | halt }
    if (%colord3rand == off) { set %colord3rand on | halt }
  }
}
on *:DIALOG:input7:edit:*: {
  if ($did == 8) { set %leftdesign $did(8).text }
  if ($did == 9) { set %rightdesign $did(9).text }
}
on *:DIALOG:input7:init:0: {
  if (%leftdesign != $null) { did -a input7 8 %leftdesign }
  if (%rightdesign != $null) { did -a input7 9 %rightdesign }
  if (%colord3rand == $null) { set %colord3rand off }
  if (%colord3rand == on) { did -c input7 11 }
}
