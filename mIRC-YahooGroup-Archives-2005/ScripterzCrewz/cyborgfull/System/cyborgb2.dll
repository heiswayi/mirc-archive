on *:text:*:%gamechannel:{
  if (%bartender == on)  {
    if ($1 == !order) { .notice $nick Type !Drink to get a drink, Type !Food to get food. }
    if ($1 == !Drink) { .notice $nick Type !Milk for milk, Type !Coffee for coffee, Type !Whiskey for whiskey, Type !Lemonade for lemonade, Type !Water for water.  For our adult selection type !adult  }
    if ($1 == !Lemonade) { msg # $nick has just ordered Lemonade from Chuckdog's bar }
    if ($1 == !Milk) { describe # hands $nick a nice hot milk 4[1,11__4]D from Chuckdog's bar }
    if ($1 == !coffee) { describe # hands $nick a fresh cup of coffee 4[1,5__4]D from Chuckdog's bar }
    if ($1 == !Water) { msg # $nick has just ordered 11,12Walter1 from Chuckdog's bar  }
    if ($1 == !adult) { .notice $nick Ooh...u think u an A-dult, lets see if u can stand it...Type, !Vodka for a shot of Vodka, Type !Beer for a beer list  }
    if ($1 == !Vodka) { msg # WOW, looks like $nick is gonna drain thier brain cells, they just ordered a VODKA!! | goto end }
    if ($1 == !beer) { .notice $nick You have just gotten into Chuckdogs beer stash, Lets see some ID, nevermind that, what'd ya like, Type !Bud for a Budweiser, Type !Coors for an ice cold Coors, Type !Sam for a Samuel Adams, Type !Rollin Rock for a Rolling Rock...Thank you for your order  }
    if ($1 == !food) { .notice $nick Here is your menu, Type !Wings for hot wings [all are suicides], type !Spam-Burger for a Spam burger Hamburger, type !Ribs for Ribs. }
    if ($1 == !bud) { describe # pours a nice, cold  0,140,4-8,4BuD0,4-0,14 12 into a glass, and slides the 12~~~~5,8 [ll]12,0D12 to12 $nick }
    if ($1 == !coors) { describe # hands a nice cold  0,140,1-16,15coors0,1-0,14 12 into a glass, and slides the 12~~~~5,8 [ll]1,0D12 to12 $nick }
    if ($1 == !Rolling) && ($2 == Rock) { msg # $nick has just ordered a Rolling Rock  }
    if ($1 == !sam) { msg # $nick has just ordered a Samuel Adams }
    if ($1 == !wings) { msg # $nick has just ordered a plate filled with SuIcIdE hOtWiNgS!! | describe # hands $nick a bucket of hand-wipes and water. | goto end }
    if ($1 == !Spam-Burger) { msg # $nick has just ordered a Spam Burger Hamburger }
    if ($1 == !ribs) { msg # $nick has just ordered a Bucket of Ribs  }
    if ($1 == !whiskey) { describe # pours $nick a big Bottle of Jack Daniels... }
  }
  if (%spin == on) { if ($1 == !spin) { set %spinnick $nick(#,$rand(1,$nick(#,0))) | msg #  $nick spins the bottle and it lands on %spinnick $+ . } }
  if (%8ball == on) { 
    if ($1 == !8ball) && (yes isin $2) { msg $chan  $+ $nick $+ , The 8Ball says: $read $mircdirtext\8ball.txt } 
    if ($1 == !8ball) && (no isin $2) { msg $chan  $+ $nick $+ , The 8Ball says: $read $mircdirtext\8ball.txt }
  }
  if (%guessnum == on) {
    inc %guesses
    if ($2 == %rnum ) { msg $chan $logo $nick has guessed the right number!! It took y'all %guesses guesses | number-off }
    if ($2 < %rnum ) { msg $chan $logo $nick $+ , nope you are too low. }
    if ($2 > %rnum ) { msg $chan $logo $nick $+ , nope you are too high! }
  }
  if (%pole == on) {
    if ($1 == !yes) { inc %yes }
    if ($1 == !no) { inc %no  }
    if ($1 == !result) { msg $chan $logo heres the current result for " $+ %pollquestion $+ ": yes: $+ %yes no: $+ %no }
  }
  if (%slot == on) {
    if ($1 == !slots) { 
      set %smn $rand(1,9) | set %smo $rand(1,9) | set %smp $rand(1,9) 
      msg # $logo $nick got a: %smn $+ , %smo $+ , and a %smp 
      if (( %smn == 7 ) && ( %smo == 7 ) && ( %smp == 7 )) { msg $chan $logo $nick has just gotten 777! }
      else { halt }
    }
  }
  :end
}
alias pollvote { msg %gamechannel $logo Pole booth is still Activated, The question is " $+ %pollquestion $+ " to vote for your answer, type "!Yes" or "!No" }
alias 8ball-on { msg %gamechannel $logo The 8Ball is Activated Type !8Ball [question here]to ask a question  | set %8ball on }
alias 8ball-off { msg %gamechannel $logoThe 8Ball is Deactivated |  set %8ball off }
alias Number-ON {
  set %rnum $rand(1,1000)
  MSG %gamechannel  $logo Guess The Number is Activated type !guess [number] to guess 
  set %guessnum on
}
alias Number-OFF {
  unset %rnum
  msg %gamechannel $logo Guess The Number is Deactivated 
  set %guessnum off
  unset %guesses
}
alias /slot-on {
  set %slot on
  set %smn $rand(1,9)
  set %smo $rand(1,9)
  set %smp $rand(1,9)
  msg %gamechannel $logo The Slot Machine is Activated type !slots to play 
}
alias /slot-off {
  unset %smn | unset %smo | unset %smp
  msg %gamechannel $logo The Slot Machine is Deactivated 
  set %slot off
}
