on *:DIALOG:findyourown:*:*:{
  if ($devent == init) {
    set %cdsp 1
    update.spychan
    if (%spybot != $null) { did -a findyourown 10 %spybot } 
    if (%spybotpass != $null) { did -a findyourown 11 %spybotpass }
    if (%spamrecordcount != $null) { did -a findyourown 21 %spamrecordcount }
    if (%spamrecordcount == $null) { did -b findyourown 21 }
    if (%spycycletime != $null) { did -a findyourown 24 %spycycletime }
    if (%setspyconnect == $null) { set %setspyconnect off }
    if (%setspyconnect == on) { did -c findyourown 19 }
    if (%inviteshit == $null) { set %inviteshit off }
    if (%inviteshit == on) { did -c findyourown 20 }
    if (%agentlog == $null) { set %agentlog off }
    if (%agentlog == on) { did -c findyourown 26 }
  }
  if ($devent == edit) {
    if ($did == 10) { set %spybot $did(10).text }
    if ($did == 11) { set %spybotpass $did(11).text }
    if ($did == 13) { set %spytempchan $did(13).text }
    if ($did == 21) { set %spamrecordcount $did(21).text }
    if ($did == 24) { set %spycycletime $did(24).text }
  }
  if ($devent == sclick) {
    if ($did == 3) { _webteam }
    if ($did == 4) { ifelsenext | launchbestwar }
    if ($did == 5) { offred:x | timerspy }
    if ($did == 6) { findyourownup }
    if ($did == 13) { set %setup.spychan.selected $did($dname,13,$did(13).sel) }
    if ($did == 14) {
      set %spychan %spychan $+ %spytempchan $+ ,
      update.spychan
    }
    if ($did == 15) {
      set -u1 %setup.rc %setup.spychan.selected $+ ,
      set %spychan $remove(%spychan,%setup.rc)
      update.spychan
    }
    if ($did == 16) { unset %spychan | update.spychan }
    if ($did == 19) {
      if (%setspyconnect == on) { set %setspyconnect off | halt }
      if (%setspyconnect == off) { set %setspyconnect on | halt }
    }
    if ($did == 20) {
      if (%inviteshit == on) { set %inviteshit off | did -b findyourown 21 | halt }
      if (%inviteshit == off) { set %inviteshit on | did -e findyourown 21 | halt }
    }
    if ($did == 26) {
      if (%agentlog == on) { set %agentlog off | halt }
      if (%agentlog == off) { set %agentlog on | halt }
    }
    if ($did == 27) { /rlevel domain }
    if ($did == 28) { /rlevel Ginviter }
  }
}
alias -l update.spychan {
  did -r findyourown 13
  set %temp.total $count(%spychan,$chr(44))
  set %temp.count 0
  :start
  inc %temp.count 1
  did -a findyourown 13 $gettok(%spychan,%temp.count,44)
  if (%temp.count < %temp.total) { goto start }
}
