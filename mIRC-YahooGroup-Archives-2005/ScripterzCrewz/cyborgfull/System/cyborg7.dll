alias findyourownset { if ($dialog(findyourown) == $null) dialog -m findyourown findyourown | findyourownopen }
alias findyourownclose {
  :reduce
  if (%cdsp <= 10) { dialog -c findyourown | halt }
  dec %cdsp 3 | dialog -s findyourown -1 -1 290 %cdsp | goto reduce
}
alias findyourownopen {
  :increase
  if (%cdsp >= 310) { dialog -s findyourown -1 -1 290 310 | halt }
  inc %cdsp 3 | dialog -s findyourown -1 -1 290 %cdsp | goto increase
}
alias findyourownup { set %cdsp 310 | dialog -s findyourown -1 -1 290 310 | .timerfindyourownclose 1 0 findyourownclose }
