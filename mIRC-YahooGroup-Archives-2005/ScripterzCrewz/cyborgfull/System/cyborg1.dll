    if ($did == 12) { set %channelvoice $did(12).text }
    if ($did == 19) { set %nickvoicetemp $did(19).text }
    if ($did == 38) { set %nickoptemp $did(38).text }
  }
  if ($devent == sclick) {
    if ($did == 3) { _webteam }
    if ($did == 7) { set %setup.voicechannel.selected $did($dname,7,$did(7).sel) }
    if ($did == 8) {
