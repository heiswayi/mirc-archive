on *:dialog:autolist:*:*:{
  if ($devent == init) {
    update.voicechannel
    update.voicenick
    update.opnick
    update.opchannel
    update.nickvoice
    update.nickop
    if (%autovoice == $null) { set %autovoice off }
    if (%autovoice == on) { did -c autolist 6 }
    if (%autoop == $null) { set %autoop off }
    if (%autoop == on) { did -c autolist 25 }
    if (%channelop != $null) { did -a autolist 31 %channelop }
    if (%channelvoice != $null) { did -a autolist 12 %channelvoice }
  }
  if ($devent == edit) {
    if ($did == 7) { set %voicetempchan $did(7).text }
    if ($did == 26) { set %optempchan $did(26).text }
    if ($did == 13) { set %voicetempnick $did(13).text }
