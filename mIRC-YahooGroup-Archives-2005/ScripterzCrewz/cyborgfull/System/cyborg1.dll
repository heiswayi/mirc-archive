on *:DIALOG:awaysys:sclick:*: {
  if ($did == 11) { aaway | did -o awaysys 6 1 Currently away, reason is %away $+ , log is %log $+ , trigger away after %awaytrigger secs }
  if ($did == 12) { backaway | did -o awaysys 6 1 Return from away }
  if ($did == 14) { set %awaycomm $did(14) | set %altnick $did(14) | did -o awaysys 18 1 %awaycomm $+ -away is ready | did -o awaysys 6 1 Preparing to away from chatting: Reason: %awaycomm Log: %log Auto-away nick: %awaynick }
  if ($did == 16) { set %awaycomm $+ away | did -o awaysys 6 1 Currently away, reason is %away $+ , log is %log $+ , trigger away after %awaytrigger secs }
  if ($did == 19) { backaway | did -o awaysys 6 1 Return from away }
  if ($did == 26) {
    if (%autoaway == on) { set %autoaway off | .timer(autoaway) off | did -o awaysys 6 1 Auto-away: %autoaway in %autoawaytime secs. Log: %log Auto-away nick: %awaynick Trigger: %awaytrigger secs | halt }
    if (%autoaway == off) { set %autoaway on | aaway.check | did -o awaysys 6 1 Auto-away: %autoaway in %autoawaytime secs. Log: %log Auto-away nick: %awaynick Trigger: %awaytrigger secs | halt }
  }
  if ($did == 3) { _webteam }
  if ($did == 4) { awaysysup }
  if ($did == 27) {
    if (%log == on) { set %log off | did -o awaysys 6 1 Auto-away: %autoaway in %autoawaytime secs. Log: %log Auto-away nick: %awaynick Trigger: %awaytrigger secs | halt }
    if (%log == off) { set %log on | did -o awaysys 6 1 Auto-away: %autoaway in %autoawaytime secs. Log: %log Auto-away nick: %awaynick Trigger: %awaytrigger secs | halt }
  }
  if ($did == 28) {
