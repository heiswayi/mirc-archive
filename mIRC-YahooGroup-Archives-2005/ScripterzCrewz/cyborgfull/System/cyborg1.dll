on *:DIALOG:kicks:init:*: {
  set %kickreason $read $mircdirText\kick.txt
  did -a kicks 7 %kickreason
  did -a kicks 7 Get out
  did -a kicks 7 Do not flood
  did -a kicks 7 annoying
  did -a kicks 7 Inv/Spam is not allowed
  did -a kicks 7 evaporate your self
  did -a kicks 7 leave me alone
  did -a kicks 7 shut up
  did -a kicks 12 %banmask
  did -a kicks 12 1
  did -a kicks 12 2
  did -a kicks 12 3
  did -a kicks 12 4
  did -a kicks 12 5
  did -a kicks 12 6
  did -a kicks 12 7
  did -a kicks 12 8
  did -a kicks 12 9
  did -a kicks 13 %unbantimes
  did -a kicks 13 10
  did -a kicks 13 30
  did -a kicks 13 60
  did -a kicks 13 180
  did -a kicks 13 600
  did -a kicks 13 1200
  did -a kicks 13 1800
  did -a kicks 13 3600
  did -a kicks 13 7200
  did -a kicks 13 86400
  did -b kicks 13
  did -b kicks 12
  set %tempban off
}
on *:DIALOG:kicks:sclick:*: {
  if ($did == 9) { set %kickonly on | set %banonly off | set %kickban off | did -b kicks 13 | did -b kicks 12 | did -e kicks 7 }
  if ($did == 10) { set %kickonly off | set %banonly on | set %kickban off | did -b kicks 7 | did -e kicks 12 }
  if ($did == 11) { set %kickonly off | set %banonly off | set %kickban on | did -e kicks 7 | did -e kicks 12 }
  if ($did == 15) {
    if (%tempban == on) { set %tempban off | did -b kicks 13 | halt }
    if (%tempban == off) { set %tempban on | did -e kicks 13 | halt }
  }
  if ($did == 5) {
    if (%kickonly == on) { kick %chankicks %nicktokicks %kickreason | goto end }
    if (%banonly == on) {
      if %tempban == on) { ban -u $+ %unbantimes %chantokicks %nicktokicks %banmask | goto end }
      ban %chankicks %nicktokicks %banmask | goto end
    }
    if (%kickban == on) {
      if %tempban == on) { kick %chankicks %nicktokicks %kickreason | ban -u $+ %unbantimes %chantokicks %nicktokicks %banmask | goto end }
      kick %chankicks %nicktokicks %kickreason | ban %chankicks %nicktokicks %banmask | goto end
    }
  }
  if ($did == 7) { set %kickreason $did($dname,7,$did(7).sel) }
  if ($did == 12) { set %banmask $did($dname,12,$did(12).sel) }
  if ($did == 13) { set %unbantimes $did($dname,13,$did(13).sel) }
  :end 
}
