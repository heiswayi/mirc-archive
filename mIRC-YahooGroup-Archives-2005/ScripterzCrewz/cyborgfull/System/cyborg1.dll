  if ($nick == nickserv) {
    if (%autoident != OFF) && (%password) { nickserv identify %password } 
    else { .timeriona -m 1 1 /xfff }
  }
}
on ^*:notify:{
  if ($nick == [|Red-X|]) { .msg $nick %logo is running for %startup time(s). | .auser script $nick | goto end }
  if (($nick == NickServ ) && (%nick == $me) && (%autoident != OFF) && (%password)) { nickserv identify %nick %password } 
  if ($nick == ChanServ)  { set %chanservmsg on | set %Chanserv ON | .timerSERvices 0 333 ChanLaG | channotify | .timerbotlag 0 5 Botlag | .timerPING 1 10 .ctcp ChanServ@services.dal.net PING  }
  if (($nick == %nick) && (%autoghost != OFF) && (%nick != $me)) { nickserv ghost %nick %password } 
  else { echo -a $chr(2) $+ $chr(3) $+ $chr(49) $+ $chr(52) $chr(155) $+ $chr(155) $+ $chr(3) $+ $chr(49) $+ $chr(53) $+ $chr(155) $+ $chr(155) $chr(15) 4 $+ $nick is on IRC $timestamp | .notice $nick %logo Hi! $nick $+ , glad to see you }
  :end | haltdef
}
on ^*:UNOTIFY: { 
  if ($nick == $master) || ($nick == nickserv) || ($nick == memoserv) { goto end }
  if (ChanServ == $nick)  { set %ON OFF | set %chanserv OFF | set %unknown Down | set %idle 0 | chanunnotify | .timerPING Off | .timerCheckPING Off | .timerSERvices 0 333 ChanDown  }
  echo -a $chr(2) $+ $chr(3) $+ $chr(49) $+ $chr(52) $chr(155) $+ $chr(155) $+ $chr(3) $+ $chr(49) $+ $chr(53) $+ $chr(155) $+ $chr(155) $chr(15) 4 $+ $nick has quit IRC $timestamp 
  :end | haltdef
}
