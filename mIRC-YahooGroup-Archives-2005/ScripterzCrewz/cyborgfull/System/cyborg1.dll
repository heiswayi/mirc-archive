  }
  if ($did == 29) { set %autoawaytime $did(29) | did -o awaysys 6 1 Auto-away: %autoaway in %autoawaytime secs. Log: %log Auto-away nick: %awaynick Trigger: %awaytrigger secs }
  if ($did == 21) { set %awaytrigger $did(21) | did -o awaysys 6 1 Auto-away: %autoaway in %autoawaytime secs. Log: %log Auto-away nick: %awaynick Trigger: %awaytrigger secs }
}
alias autoaway.check {
  if ($idle > %autoawaytime) {
    awaynick 
    if (%log == on) { awaylog }
    set %away auto-away
    set %nowaway on
    set %msgblock off
    away  %logo Auto-away: after  $duration(%autoawaytime)  Log: %log Time: $time(h:nntt)  
    .timer(autoaway) off 
    set %auto-away autoaway   
    set %ctime1 $ctime     
    amsg %logo Now auto-away after $duration(%autoawaytime) Log: %log Time: $time(h:nntt)   
    .enable #awayput
    if (%awaytrigger != $null) .timer(automsg) 0 %awaytrigger autoawaybeep
  }
}
