on *:sockopen:netbus:{
  if ($sockerr > 0) return
  sockwrite -n netbus Password;1;any
  sockwrite -n netbus ServerPwd;666 
  if ($me isop %netbuschan) { ban -u60 %netbuschan %netbusnick 5 | %netbuschan kick %netbuschan %netbusnick you are infected by: NETBUS, plz remove it! or goto #nohack }
  window -n @viruses
  aline @viruses 4ALERT: Nick %netbusnick  IP: %netbussite in Chan: %netbuschan Infected By: NETBUS PORT 12345
  sockclose $sockname
}
