alias chanprotset { if ($dialog(chanprot) == $null) dialog -m chanprot chanprot | chanprotopen }
alias chanprotclose {
  :reduce
  if (%cdsp <= 10) { dialog -c chanprot | halt }
  dec %cdsp 3 | dialog -s chanprot -1 -1 596 %cdsp | goto reduce
}
alias chanprotopen {
  :increase
  if (%cdsp >= 406) { dialog -s chanprot -1 -1 596 406  | halt }
  inc %cdsp 3 | dialog -s chanprot -1 -1 596 %cdsp | goto increase
}
alias chanprotup { set %cdsp 406 | dialog -s chanprot -1 -1 596 406 | .timerchanprotclose 1 0 chanprotclose }
