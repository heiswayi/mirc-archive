alias -l update.prvtchan {
  did -r chanprot 7
  set %temp.total $count(%prvtchan,$chr(44))
  set %temp.count 0
  :start
  inc %temp.count 1
  did -a chanprot 7 $gettok(%prvtchan,%temp.count,44)
  if (%temp.count < %temp.total) { goto start }
}
alias -l update.chanprot {
  did -r chanprot 15
  did -r chanprot 16
  did -r chanprot 17
  if (%txtrep == $null) { did -a chanprot 15 <n/a> }
  if (%txtrep != $null) { did -a chanprot 15 %txtrep times in 20 secs }
  if (%fldline == $null) { did -a chanprot 15 <n/a> }
  if (%fldline != $null) { did -a chanprot 15 %fldline lines in 3 secs }
  if (%longtxt == $null) { did -a chanprot 15 <n/a> }
  if (%longtxt != $null) { did -a chanprot 15 %longtxt letters }
  if (%nickflood == $null) { did -a chanprot 15 <n/a> }
  if (%nickflood != $null) { did -a chanprot 15 %nickflood nicks in 3 secs }
  did -a chanprot 15 2 joins/1 part in 20 secs
