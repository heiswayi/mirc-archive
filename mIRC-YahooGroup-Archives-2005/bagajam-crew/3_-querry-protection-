.window -eknl12 @Msg 0 0 718 504 @Msg Times New Roman Bold 10

.aline -l 4 @Msg $nick 

.echo @Msg 0[- 12Nick 0-] 4,12 $nick  2: $1-

.echo -a 12»» 4I15ncoming 4C15hat 4R15equested 12«« 15[[8,7 $+ $nick $+ 15]]

}

